struct Params {
	address: array[int],
	payload: any,
	sender: string
}
service PremiumService : (any) -> void;
service BasicService : (any) -> void;
fn function(isPremiumUser: bool, par: any) -> void {
	if ( Params.sender == "ciao" ) {
		call PremiumService( par );
	} else {
		call BasicService( par );
	}
}